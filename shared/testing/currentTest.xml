<tests>
	<test name='buildAssocTest1'>

		<startcap name = 'sc' type = 'Set Number' />

		<endcap name = 'ec' expression = 'buildAssoc (add 2) sc' />

		<messages>
			<set scname = 'sc'>
				<number value = '1' />
			</set>
			<set scname = 'sc'>
				<number value = '2' />
			</set>
		</messages>

		<expectedmessages>
			<set ecname = 'ec'>
				<number value = '1' />
				<number value = '3' />
			</set>
			<set ecname = 'ec'>
				<number value = '2' />
				<number value = '4' />
			</set>
		</expectedmessages>

	</test>

</tests>