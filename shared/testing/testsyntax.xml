<tests>

	<test name='foldTest1'>
		<startcap name='sc' type='set number' />
	
		<messages>
			<set scname = 'sc'>
				<number value='2' />
			</set>
			<set scname = 'sc'>
				<number value='5' />
			</set>
		</messages>
	
		<endcap name = 'ec' expression = 'fold add 0 sc' />

		<expectedmessages>
			<set ecname = 'ec' >
				<number value = '7' />
			</set>
		</expectedmessages>
	
		<messages>
			<set scname = 'sc'>
				<number value='3' />
			</set>
			<set scname = 'sc'>
				<number value='5' />
			</set>
			<remove scname = 'sc'>
				<number value='2' />
			</remove>
		</messages>
	
		<expectedmessages>
			<set ecname = 'ec' >
				<number value = '10' />
			</set>
			<set ecname = 'ec' >
				<number value = '9' />
			</set>
		</expectedmessages>
	
		<removecap name = 'ec' />
	</test>

</tests>