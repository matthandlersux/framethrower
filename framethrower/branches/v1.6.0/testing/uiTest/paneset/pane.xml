<f:template name="testTemplate"
	xmlns:f="http://www.worldmerge.com/2008/xsl"
	xmlns:xsl="http://www.w3.org/1999/XSL/Transform"
	xmlns:html="http://www.w3.org/1999/xhtml"
	xmlns:svg="http://www.w3.org/2000/svg">
	
	<f:param name="newPane" type="a -> Unit JS" />
	<f:param name="closePane" type="Unit JS" />
	<f:param name="pane" type="UI.pane" />
	<f:param name="width" type="Number" />
	<f:param name="height" type="Number" />
	
	<f:derive name="asSet" d="UI.pane~UI.pane.set pane" />
	<f:derive name="asPane" d="UI.pane~UI.pane.pane pane" />
	<f:derive name="asTimeline" d="UI.pane~UI.pane.timeline pane" />
	
	<f:each select="$asSet">
		<f:thunk url="set.xml">
			<f:with-param name="width" select="$width" />
			<f:with-param name="height" select="$height" />
			<f:with-param name="paneset" select="$asSet" />
		</f:thunk>
	</f:each>
	<f:each select="$asPane">
		<f:thunk url="../pane/pane.xml">
			<f:with-param name="pane" select="$asPane" />
			<f:with-param name="newPane" select="$newPane" />
			<f:with-param name="closePane" select="$closePane" />
			<f:with-param name="width" select="$width" />
			<f:with-param name="height" select="$height" />
		</f:thunk>
	</f:each>
	<f:each select="$asTimeline">
		<f:thunk url="../timeline/timeline.xml">
			<f:with-param name="timeline" select="$asTimeline" />
			<f:with-param name="newPane" select="$newPane" />
			<f:with-param name="closePane" select="$closePane" />
			<f:with-param name="width" select="$width" />
			<f:with-param name="height" select="$height" />
		</f:thunk>
	</f:each>
		
</f:template>