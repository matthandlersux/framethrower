<f:transaction name="transtest"
	xmlns:f="http://www.filmsfolded.com/xsl/ui"
    xmlns:xsl="http://www.w3.org/1999/XSL/Transform">

	<f:param name="object" />
	<f:param name="destination" />
	
	<xsl:template>
		<transaction>
			
			<f:perform absurl="xml/api/moveObject.xml">
				<f:with-param name="object" value-id="{$object/@id}" />
				<f:with-param name="destination" value-id="{$destination/@id}" />
			</f:perform>
			
		</transaction>
	</xsl:template>
</f:transaction>