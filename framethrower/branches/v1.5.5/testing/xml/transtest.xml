<f:transaction name="transtest"
	xmlns:f="http://www.filmsfolded.com/xsl/ui"
    xmlns:xsl="http://www.w3.org/1999/XSL/Transform">

	<f:param name="parentSituation" />
	<f:param name="ind0" />
	<f:param name="ind1" />
	
	<xsl:template>
		<transaction>
			
			<f:perform absurl="xml/api/correspond.xml">
				<f:with-param name="object1" value-id="{$ind0/@id}" />
				<f:with-param name="object2" value-id="{$ind1/@id}" />
			</f:perform>
			
		</transaction>
	</xsl:template>
</f:transaction>