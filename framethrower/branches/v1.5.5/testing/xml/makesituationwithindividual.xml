<f:transaction name="makeSituationWithIndividual"
	xmlns="http://www.w3.org/1999/xhtml"
	xmlns:f="http://www.filmsfolded.com/xsl/ui"
    xmlns:xsl="http://www.w3.org/1999/XSL/Transform">

	<f:param name="parentSituation" />
	
	<xsl:template>
		<transaction>
			<f:perform absurl="xml/api/makeSituation.xml" prefix="sit1">
				<f:with-param name="parentSituation" value-id="{$parentSituation/@id}" />
			</f:perform>
			<intact with-var="sit1.obj.o" prop="content" action="set">
				<f:with-param><f:string value="Test Situation" /></f:with-param>
			</intact>
		</transaction>
	</xsl:template>
</f:transaction>