<situation name="ont" returnas="ont"
	xmlns:f="http://www.filmsfolded.com/xsl/ui" xmlns:xsl="http://www.w3.org/1999/XSL/Transform" xmlns:html="http://www.w3.org/1999/xhtml" xmlns:svg="http://www.w3.org/2000/svg">
	<content>
		Ontological Layer
	</content>
	<relation name="ofType" returnas="ofType">
		<content>
			<f:relationcontent>
				<f:name value="ofType" />
				<f:pseudo>
					<f:role role="object"/>
					<f:verb value="is of type"/>
					<f:role role="type"/>
				</f:pseudo>
			</f:relationcontent>
		</content>
	</relation>
	<individual name="typeType" returnas="typeType" type="typeType">
		<content>
			Types
		</content>	
	</individual>
	<relation name="corresponds" returnas="corresponds" />
	<relation name="childrelation1" returnas="childrelation1">
		<content>
			<f:relationcontent>
				<f:name value="childRel1" />
				<f:pseudo>
					<f:role role="parent"/>
					<f:verb value="is parent of"/>
					<f:role role="child"/>
				</f:pseudo>
			</f:relationcontent>
		</content>
	</relation>
		
		
	<relation name="childrelation2" returnas="childrelation2">
		<content>
			<f:relationcontent>
				<f:name value="childRel2" />
				<f:pseudo>
					<f:role role="parent"/>
					<f:verb value="is parent of"/>
					<f:role role="child"/>
				</f:pseudo>
			</f:relationcontent>
		</content>
	</relation>
	
	
	
	<situation name="rw" returnas="rw">
		<content>
			Real Life
		</content>
	</situation>
	
	<individual name="ind1" type="typeType">
		<content>
			ind1
		</content>
	</individual>
	
	<individual name="ind2" type="typeType">
		<content>
			ind2
		</content>
	</individual>
	
	<individual name="ind3" type="typeType">
		<content>
			ind3
		</content>
	</individual>
	
	<infon relation="childrelation1">
		<arc role="parent" arg="rw"/>
		<arc role="child" arg="ind1"/>
	</infon>
	
	<infon relation="childrelation1">
		<arc role="parent" arg="rw"/>
		<arc role="child" arg="ind2"/>
	</infon>
	
	<infon relation="childrelation2">
		<arc role="parent" arg="rw"/>
		<arc role="child" arg="ind3"/>
	</infon>
	
</situation>
