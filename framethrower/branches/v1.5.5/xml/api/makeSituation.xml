<f:transaction
	xmlns:f="http://www.filmsfolded.com/xsl/ui"
    xmlns:xsl="http://www.w3.org/1999/XSL/Transform">

	<f:param name="parentSituation" />
	<xsl:template>
		<transaction>
			<f:perform url="helper/makeobject.xml" prefix="obj">
				<f:with-param name="type"><f:string value="kernel.situation" /></f:with-param>
				<f:with-param name="parentSituation"><xsl:copy-of select="$parentSituation" /></f:with-param>
			</f:perform>
			<f:return as="o" value="obj.o" />
		</transaction>
	</xsl:template>
</f:transaction>