	<relation name="corresponds" returnas="corresponds" />
	<situation name="rw" returnas="rw">
		<content><information name="Real Life" thumbnail="real_life" /></content>

		<relation name="performs" returnas="performs">
			<content>
				<f:relationcontent>
					<f:name value="performs" />
					<f:pseudo>
						<f:role role="performer"/>
						<f:verb value="performs"/>
						<f:role role="performee"/>
					</f:pseudo>
				</f:relationcontent>
			</content>
		</relation>
		<relation name="dreams" returnas="performs">
			<content>
				<f:relationcontent>
					<f:name value="dreams" />
					<f:pseudo>
						<f:role role="dreamer"/>
						<f:verb value="dreams"/>
						<f:role role="dream"/>
					</f:pseudo>
				</f:relationcontent>
			</content>
		</relation>
		<relation name="mother">
			<content>
				<f:relationcontent>
					<f:name value="mother of" />
					<f:pseudo>
						<f:role role="mother"/>
						<f:verb value="mother of"/>
						<f:role role="son"/>
					</f:pseudo>
				</f:relationcontent>
			</content>
		</relation>
		
		<individual name="arnold" type="personType">
			<content><information name="Arnold Schwartzenegger" thumbnail="arnold_real" /></content>
		</individual>
		
		<individual name="stallone" type="personType">
			<content><information name="Sylvester Stallone" thumbnail="stallone_real" /></content>
		</individual>
		
		<individual name="la" type="locationType">
			<content><information name="Los Angeles" thumbnail="los_angeles_real" special="2" /></content>
		</individual>
		
		
		<situation name="lah" type="movieType">
			<content><information name="Last Action Hero" thumbnail="movie_last_action_hero_real" /></content>
			
			<individual name="arnoldlah" type="personType">
				<content><information name="Arnold Schwartzenegger" thumbnail="arnold_lah" /></content>
			</individual>
			<individual name="danny" type="personType">
				<content><information name="Danny Madigan" thumbnail="danny_madigan_lah" /></content>
			</individual>
			
			<situation name="jackslateriv" type="movieType">
				<content><information name="Jack Slater IV" thumbnail="movie_jack_slater_iv_lah" /></content>
				<individual name="jackslater" type="personType">
					<content><information name="Jack Slater" thumbnail="jack_slater_jsiv" /></content>
				</individual>
				
				<situation name="terminatorjs" type="movieType">
					<content><information name="Terminator 2: Judgement Day" thumbnail="movie_terminator_two_jsiv" /></content>

					<individual name="cyborgjs" type="cyborgType">
						<content><information name="The Terminator" thumbnail="cyborg_jsiv" /></content>
					</individual>
				</situation>
				
				<infon relation="performs">
					<arc role="performer" arg="stallone" />
					<arc role="performee" arg="cyborgjs" />
				</infon>
			</situation>
			
		</situation>
		
		<situation name="tmov" type="movieType">
			<content><information name="Terminator 2: Judgement Day" thumbnail="movie_terminator_two_judgement_day_real" /></content>
			
			<individual name="sc" type="personType">
				<content><information name="Sarah Connor" thumbnail="sarah_connor_terminator_2" /></content>
			</individual>
			<individual name="jc" type="personType">
				<content><information name="John Connor" thumbnail="john_connor_terminator_2" /></content>
			</individual>
			
			<individual name="cyborg" type="cyborgType">
