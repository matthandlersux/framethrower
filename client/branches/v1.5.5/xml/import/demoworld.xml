					<content><information name="Los Angeles" special="2" thumbnail="los_angeles_real" /></content>
				</individual>
				<individual name="apsc" type="personType">
					<content><information name="Sarah Connor" thumbnail="sarah_connor_terminator_2" /></content>
				</individual>
			</situation>
			
			<infon relation="mother">
				<arc role="mother" arg="sc" />
				<arc role="son" arg="jc" />
			</infon>
			<infon relation="dreams">
				<arc role="dreamer" arg="sc" />
				<arc role="dream" arg="apocalypse" />
			</infon>
		</situation>
		
		<infon relation="performs">
			<arc role="performer" arg="arnold" />
			<arc role="performee" arg="cyborg" />
		</infon>
		<infon relation="performs">
			<arc role="performer" arg="arnold" />
